::-webkit-input-placeholder {
  color: #a0b0b7;
}

:-moz-placeholder {
  color: #a0b0b7;
}

::-moz-placeholder {
  color: #a0b0b7;
}

:-ms-input-placeholder {
  color: #a0b0b7;
}

::placeholder {
  color: #a0b0b7;
}

body {
  position: relative;
  background: url(../../images/login/login-bg.jpg) center center no-repeat;
  height: 100%;
  min-height: 420px;
  -webkit-background-size: cover;
  background-size: cover;
}

a:hover {
  border-bottom: 1px solid;
}

.login {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 720px;
  height: 420px;
}

.platform {
  height: 90px;
  color: #fff;
  font-size: 22px;
  line-height: 40px;
}

.logo {
  position: relative;
  background: url(../../images/login/logo.png);
  display: inline-block;
  *display: inline;
  *zoom: 1;
  width: 150px;
  height: 40px;
  vertical-align: middle;
  margin-right: 20px;
}

.logo:after {
  content: "";
  position: absolute;
  width: 0;
  border-left: 1px solid;
  right: 0;
  top: 0;
  bottom: 0;
  height: 25px;
  margin: auto;
}

.login-main {
  height: 330px;
}

.login-account,
.login-qrcode {
  float: left;
  padding: 30px 45px;
  height: 90%;
}

.login-account {
  background: #fff;
  width: 400px;
}

.login-account .login-type {
  color: #00c5dc;
}

.register-link {
  float: right;
}

.login-type {
  font-size: 18px;
  margin-bottom: 15px;
}

.login-qrcode {
  width: 320px;
  background: #00c5dc;
  color: #fff;
  text-align: center;
}

.login-form {
  padding: 10px 0;
}

.login-fieldset {
  border: 1px solid #e1e5e9;
  margin-bottom: 35px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
  padding: 0;
}

.login-fieldset legend {
  display: none;
}

.login-input-block {
  display: block;
  margin-left: 40px;
  margin-bottom: 0;
  border-left: 1px solid #e1e5e9;
  position: relative;
}

.login-input-block .login-input-icon {
  float: left;
  width: 40px;
  height: 30px;
  margin-top: 10px;
  margin-left: -40px;
  text-align: center;
  line-height: 30px;
  color: #8499a3;
  font-size: 22px;
}

.login-input-block input {
  height: 40px;
  line-height: 24px;
  padding: 3px 10px;
  width: 100%;
  border: 0;
  color: #333;
}

.login-input-block input:focus {
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
}

.login-input-block .login-verify-btn,
.login-input-block .login-verify-img {
  position: absolute;
  right: 5px;
  top: 0;
  bottom: 0;
  margin: auto 0;
  border: 0;
  outline: 0;
}
.skin-blue .btn-primary {

    background-color: #1dbee2;
    border-color: #1cb5d7;

}
.login-input-block .login-verify-img {
  height: 32px;
  cursor: pointer;
}

.login-input-block .login-verify-btn {
  padding: 0 10px;
  line-height: 28px;
  height: 28px;
}

.login-input-block .login-verify-btn:focus {
  color: #00c5dc;
}

.login-input-block .login-verify-btn:disabled {
  color: #fff;
}

.login-submit-btn {
  background: #00c5dc;
  color: #fff;
  font-size: 18px;
  line-height: 50px;
  border: 0;
  width: 130px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  letter-spacing: 5px;
  text-indent: 5px;
}

.login-submit-btn:hover,
.login-submit-btn:focus {
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  background: #00bbd1;
}

.u-check__indicator {
  color: #00c5dc;
  border-color: #e1e5e9;
  font-size: 1.5em;
  vertical-align: top;
}

input:focus + .u-check__indicator {
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
}

.login-qrcode-img {
  width: 160px;
  height: 160px;
  position: relative;
  margin: auto;
}

.login-qrcode-img .qrcode {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
}

.login-qrcode-img:hover .qrcode-refresh {
  display: table;
}

.login-qrcode-img .qrcode-refresh {
  background: rgba(0, 0, 0, 0.5);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#7f000000', endColorstr='#7f000000');
  position: relative;
  width: 100%;
  height: 100%;
  display: none;
}

:root .login-qrcode-img .qrcode-refresh {
  -webkit-filter: none\9;
  filter: none\9;
}

.login-qrcode-img .inner {
  display: table-cell;
  vertical-align: middle;
  width: 100%;
  height: 100%;
}

.login-qrcode-img p {
  margin-bottom: 10px;
}

.login-qrcode-img .qrcode-refresh-btn {
  background: #00c5dc;
  -webkit-border-radius: 2px;
  border-radius: 2px;
  padding: 2px 20px;
}

.login-qrcode-img .qrcode-refresh-btn:hover,
.login-qrcode-img .qrcode-refresh-btn:focus {
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(102, 175, 233, 0.6);
  background: #00bbd1;
}

.toast {
  color: #fff;
  background: rgba(200, 100, 100, 0.8);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ccc86464', endColorstr='#ccc86464');
  position: fixed;
  top: 0;
  padding: 10px;
  text-align: center;
  left: 0;
  right: 0;
  width: 320px;
  margin: auto;
  -webkit-border-radius: 0 0 5px 5px;
  border-radius: 0 0 5px 5px;
  display: none;
}

:root .toast {
  -webkit-filter: none\9;
  filter: none\9;
}

.toast .message {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  text-align: left;
}
