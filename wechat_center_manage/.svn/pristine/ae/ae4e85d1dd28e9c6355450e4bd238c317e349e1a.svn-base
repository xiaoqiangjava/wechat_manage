.container,
.login-main {
  *zoom: 1;
}

.container:before,
.login-main:before,
.container:after,
.login-main:after {
  content: "";
  display: table;
}

.container:after,
.login-main:after {
  clear: both;
}

.btn {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  padding: 6px 12px;
  margin-bottom: 0;
  font-size: 14px;
  font-weight: 400;
  line-height: 1.42857143;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  -ms-touch-action: manipulation;
  touch-action: manipulation;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  background-image: none;
  border: 1px solid transparent;
  -webkit-border-radius: 4px;
  border-radius: 4px;
}
.dialog {
  position: fixed;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  z-index: 1;
  display: none;
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transition: opacity 0.2s;
  transition: opacity 0.2s;
}

.dialog__mask {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: #000;
  opacity: 0.3;
  filter: alpha(opacity=30);
}

.dialog__main {
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-box-shadow: 0 0 0 10px rgba(0, 0, 0, 0.2);
  box-shadow: 0 0 0 10px rgba(0, 0, 0, 0.2);
  -webkit-border-radius: 5px;
  border-radius: 5px;
  max-height: 100%;
  width: 360px;
  padding-top: 40px;
  padding-bottom: 20px;
}

.dialog__header {
  line-height: 40px;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}

.dialog__close {
  font: 0/0 a;
  color: transparent;
  text-shadow: none;
  background-color: transparent;
  border: 0;
  width: 40px;
  height: 40px;
  position: absolute;
  right: 0;
  top: 0;
  background: url(../../images/login/icon-dialog-close.png);
  z-index: 1;
}

.dialog__body {
  padding: 15px;
}

.dialog__footer {
  text-align: center;
  padding-top: 20px;
  padding-bottom: 20px;
}

.dialog__footer .btn {
  margin: 0 5px;
  min-width: 80px;
}

.dialog--show {
  display: block;
  opacity: 1;
  filter: alpha(opacity=100);
}

.msg__main {
  width: 360px;
}

.msg__body {
  text-align: center;
}

.msg__message {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  text-align: left;
  font-size:14px;
}
