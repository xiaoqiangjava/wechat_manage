<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!--引入jstl标签  -->
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib uri = "http://java.sun.com/jsp/jstl/fmt" prefix = "fmt" %>
<%@ taglib uri = "http://java.sun.com/jsp/jstl/functions" prefix = "fn" %>
<!-- 请求路径配置 -->
<c:set var="ctx" value="${contextPath_project}"/>
<c:set var="ctxStatic" value="${pageContext.request.contextPath}/resource"/>
<c:set var="ctxStaticPre" value="${contextPath_static}"/>
<!-- 公用page配置 -->
<script>
	var proCtx = "${ctx}";
	var ctxStaticPre = "${ctxStaticPre}";
</script>
<script type="text/javascript" src="${contextPath_static}/js/jquery/jquery-3.2.1.min.js"></script>
<script type="text/javascript" src="${contextPath_static}/js/layer/layer.js"></script>
<script type="text/javascript" src="${contextPath_static}/js/common/common.js"></script>